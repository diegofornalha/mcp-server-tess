{
  "name": "mcp-server-tess",
  "version": "1.0.0",
  "description": "Servidor MCP para integração com a API TESS",
  "main": "build/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon -e ts --exec \"npm run dev\"",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "tess",
    "ai",
    "api",
    "integration"
  ],
  "author": "TESS Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
